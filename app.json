{
  "expo": {
    "name": "NutriMind",
    "slug": "NutriMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/NewLogo5.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to scan barcodes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.nutrimind.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "03e6b137-e99c-48a2-9393-14c612cb69d0"
      },
      "API_BASE_URL": "https://back-c6rh.onrender.com"
    },
    "owner": "akezhaans"
  }
}
